<script>
    var qsparam = new Array(10);
    var query = window.location.search.substring(1);
    var parms = query.split('&&');

    for (var i = 0; i < parms.length; i++) {
        var pos = parms[i].indexOf('=');
        if (pos > 0) {
            var key = parms[i].substring(0, pos);
            var val = parms[i].substring(pos + 1);
            qsparam[i] = val;
        }
    }
    $(document).ready(function () {
       
        var Empid = qsparam[0];

        var jq = jQuery.noConflict();
        jq.ajax({
            type: "POST",
            url: '/EmployeeList/GetEmployeeDetails',
            data: { "Empid": Empid },
            datatype: JSON,
            success: function (response) {
                
                var RecordLength = response.length;
                if (RecordLength > 0) {
                    jq("#lblfirstName").text(response[0].empfirstname);
                    jq("#lblmiddlename").text(response[0].empmiddlename);
                    jq("#lbllastname").text(response[0].emplastname);
                    jq("#lblDOB").text(response[0].Empdofb);
                    jq("#lblGender").text(response[0].empgender);
                    jq("#lblCompanyCode").text(response[0].companycode);
                    jq("#lblcategory").text(response[0].empcategory);
                    jq("#lblempno").text(response[0].empno);
                    jq("#lblempcode").text(response[0].empcode);
                    jq("#lblaadharno").text(response[0].adharno);
                    jq("#lblpersonalno").text(response[0].emppersonalcontactno);
                    jq("#lbljoiningdate").text(response[0].Joiningdate);
                    jq("#lbldept").text(response[0].empdept);
                    jq("#lblpostname").text(response[0].emppost);
                    jq("#lbldesignation").text(response[0].Designation);
                    jq("#lblempstatus").text(response[0].empstatus);
                    jq("#lblpersonalemail").text(response[0].emppersonalemail);
                    jq("#lblofficialph").text(response[0].empofficialcontactno);
                    jq("#lblempemail").text(response[0].empemail);
                }
            },
            error: function (er) {

                console.log(er);
            }
        });
        

    });
    function BacktoMain() {
       
        window.location.href = "/EmployeeList?SearchValue=" + qsparam[1] + "";
    }

</script>

